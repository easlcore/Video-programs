{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "sourceMap": true,
      "removeComments": false,
      "jsx": "react",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "lib": [
        "dom",
        "es5",
        "es2015",
        "es2016",
        "scripthost"
      ],
      "baseUrl": ".",
      "paths": {
        "components/*": [
          "components/*"
        ],
        "containers/*": [
          "containers/*"
        ],
        "interfaces/*": [
          "interfaces/*",
          "../server/interfaces/*"
        ],
        "utils/*": [
          "utils/*"
        ]
      }
    },
    "exclude": [
      "node_modules"
    ]
  }
  